{
  "name": "federalist-uswds-gatsby2",
  "description": "A Federalist template using USWDS 2.0 + Gatsby",
  "version": "0.1.0",
  "author": "David Corwin <david.corwin@gsa.gov>",
  "dependencies": {
    "gatsby": "^2.17.11",
    "gatsby-awesome-pagination": "^0.3.5",
    "gatsby-plugin-manifest": "^2.2.27",
    "gatsby-plugin-offline": "^3.0.19",
    "gatsby-plugin-react-helmet": "^3.1.14",
    "gatsby-plugin-sass": "^2.1.22",
    "gatsby-source-filesystem": "^2.1.36",
    "gatsby-transformer-remark": "^2.6.35",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "uswds": "^2.2.1"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "federalist": "npm run federalist:build && npm run federalist:copy",
    "federalist:build": "gatsby build --prefix-paths",
    "federalist:copy": "mkdir -p _site && rm -rf _site/* && cp -r public/* _site/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/18F/federalist-uswds-gatsby2"
  }
}
